{
  "compilerOptions": {
    "target": "esnext", 
    "module": "commonjs", 
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["src/lib/*"],
      "$db":["src/db"],
      "$res/*":["src/resolvers/lib/*"]
    },
    "moduleResolution": "Node",
    "lib": [
      "ES2021.String"
    ],
    "outDir": "./dist",
    "isolatedModules": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,  
    "strict": true,
    "skipLibCheck": true 
  }
}
